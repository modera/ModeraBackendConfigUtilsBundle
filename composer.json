{
    "name": "modera/backend-config-utils-bundle",
    "type": "symfony-bundle",
    "description": "Bundle provides tools that simplify contributing your own configuration sections to 'Backend/Tools/Settings' section.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org"
        },
        {
            "name": "Sergei Vizel",
            "email": "sergei.vizel@modera.org"
        }
    ],
    "require": {
        "php": ">=7.4",
        "modera/server-crud-bundle": "^5.0",
        "modera/dynamically-configurable-mjr-bundle": "^5.0"
    },
    "require-dev": {
        "phake/phake": "^4.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": { "Modera\\BackendConfigUtilsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    }
}
